{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "strict": 0,
    "padded-blocks": 0,
    "no-underscore-dangle": 0,
    "object-shorthand": 0,
    "comma-dangle": 0,
    "func-names": 0,
    "no-var": 0,
    "vars-on-top": 0,
    "prefer-arrow-callback": 0,
    "no-unused-expressions": [2, { "allowTernary": true, "allowShortCircuit": true }],
    "global-require": 0,
    "prefer-template": 0,
    "max-len": "off"
  },
  "globals": {
    "OT": true,
    "_": true,
    "$": true,
    "axios": true,
    "crypto": true
  }
}
